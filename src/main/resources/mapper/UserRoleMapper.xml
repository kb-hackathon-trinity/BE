<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.trinity.be.user.mapper.UserRoleMapper">

    <insert id="save" parameterType="UserRole">
        insert into user_role(role, user_no)
        values (#{role}, #{ID});
    </insert>

    <select id="findUserRoleByUserNo" parameterType="int" resultType="UserRole">
        select * from user_role where user_no = #{userNo};
    </select>
</mapper>